{
    "extends": ["google", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "rules": {
        "linebreak-style": ["error", "windows"]
    },
    "globals": {
        "document": true,
        "window": true,
        "sessionStorage": true,
        "fetch": true,
        "global": true,
        "describe": true,
        "it": true,
        "beforeEach": true,
        "require": true,
        "process": true,
        "module": true
    },
    "plugins": [
        "react"
    ]
}